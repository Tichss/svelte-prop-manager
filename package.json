{
    "name": "svelte-prop-manager",
    "displayName": "Svelte Prop Manager",
    "description": "Kezeld és szerkeszd a Svelte komponens propokat könnyedén.",
    "version": "0.0.4",
    "icon": "images/icon.png",
    "publisher": "tichss",
    "engines": {
        "vscode": "^1.70.0"
    },
    "repository": "https://github.com/Tichss/svelte-prop-manager",
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "panel": [
                {
                    "id": "mySecondarySidebar",
                    "title": "Svelte Prop Manager",
                    "icon": "images/icon.svg"
                }
            ]
        },
        "views": {
            "mySecondarySidebar": [
                {
                    "type": "webview",
                    "id": "myWebviewView",
                    "name": "My Right Panel",
                    "icon": "images/icon.svg"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test",
        "build": "npm version patch --no-git-tag-version && vsce package",
        "build:ui": "cd svelte-ui && npm run build && cd.."
    },
    "devDependencies": {
        "@types/node": "^22.15.3",
        "@types/vscode": "^1.70.0",
        "typescript": "^4.8.0",
        "vsce": "^2.11.0"
    }
}
